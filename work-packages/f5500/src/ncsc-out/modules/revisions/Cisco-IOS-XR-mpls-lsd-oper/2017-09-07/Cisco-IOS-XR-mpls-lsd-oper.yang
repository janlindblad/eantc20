  augment "/ncs:devices/ncs:device/ncs:live-status" {
    container mpls-lsd-nodes {
      tailf:ncs-device-type "netconf";
      tailf:display-when "not(../../ncs:module) or ../../ncs:module[ncs:name='Cisco-IOS-XR-mpls-lsd-oper'] or (../../ncs:live-status-protocol[not(ncs:capability)]) or (../../ncs:live-status-protocol/ncs:capability/ncs:module[.='Cisco-IOS-XR-mpls-lsd-oper'])";
      description
        "Location MPLS LSD operational data";
      list mpls-lsd-node {
        key "node-name";
        description
