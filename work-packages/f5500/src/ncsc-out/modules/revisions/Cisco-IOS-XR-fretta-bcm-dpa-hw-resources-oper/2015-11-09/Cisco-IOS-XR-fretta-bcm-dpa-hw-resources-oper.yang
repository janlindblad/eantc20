            }
            container hw-resources-datas {
              description
                "OFA hw resources stats ";
              list hw-resources-data {
                key "resource";
                description
                  "Hardware resources table";
                leaf resource {
                  type Resource;
                  description
                    "Resource type";
                }
                uses live_ncs_OFA-SYSDB-HW-RESOURCE;
              }
            }
            leaf node-name {
              type xr:Node-id;
              description
                "Node ID";
            }
          }
        }
      }
    }
