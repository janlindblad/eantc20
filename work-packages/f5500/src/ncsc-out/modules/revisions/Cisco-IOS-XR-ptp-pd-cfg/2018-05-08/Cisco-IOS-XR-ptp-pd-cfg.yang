  augment "/ncs:devices/ncs:device/ncs:config" {
    container log-servo-root {
      tailf:ncs-device-type "netconf";
      tailf:display-when "not(../../ncs:module) or ../../ncs:module[ncs:name='Cisco-IOS-XR-ptp-pd-cfg']";
      description
