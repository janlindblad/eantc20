      description
        "Servo Log for Platform";
      leaf servo-event-enable {
        type boolean;
        description
          "Enable Servo change log events";
      }
    }
  }
  augment "/ncs:devices/ncs:template/ncs:ned-id/ncs:config" {
    container log-servo-root {
      description
        "Servo Log for Platform";
      leaf servo-event-enable {
        type string;
        description
          "Enable Servo change log events";
      }
    }
  }
