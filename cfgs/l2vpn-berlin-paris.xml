<config xmlns="http://tail-f.com/ns/config/1.0">
  <l2vpn-svc xmlns="lsa-urn:ietf:params:xml:ns:yang:ietf-l2vpn-svc">
    <vpn-services>
      <vpn-service>
        <vpn-id>45</vpn-id>
        <frame-delivery>
          <multicast-gp-port-mapping>static-mapping</multicast-gp-port-mapping>
        </frame-delivery>
        <ce-vlan-preservation>true</ce-vlan-preservation>
        <ce-vlan-cos-preservation>true</ce-vlan-cos-preservation>
      </vpn-service>
    </vpn-services>
    <sites>
      <site>
        <site-id>berlin</site-id>
        <devices>
          <device>
            <device-id>nok150</device-id>
            <location>center</location>
          </device>
        </devices>
        <management>
          <type>provider-managed</type>
        </management>
        <locations>
          <location>
            <location-id>center</location-id>
          </location>
        </locations>
        <vpn-policies>
          <vpn-policy>
            <vpn-policy-id>1</vpn-policy-id>
            <entries>
              <id>1</id>
              <vpn>
                <vpn-id>45</vpn-id>
              </vpn>
            </entries>
          </vpn-policy>
        </vpn-policies>
        <default-ce-vlan-id>1</default-ce-vlan-id>
        <site-network-accesses>
          <site-network-access>
            <network-access-id>1</network-access-id>
            <device-reference>nok150</device-reference>
            <connection>
              <oam>
                <md-name>1</md-name>
                <md-level>1</md-level>
              </oam>
            </connection>
            <vpn-attachment>
              <vpn-policy-id>1</vpn-policy-id>
            </vpn-attachment>
            <service>
              <svc-mtu>1500</svc-mtu>
            </service>
          </site-network-access>
        </site-network-accesses>
      </site>
      <site>
        <site-id>paris</site-id>
        <devices>
          <device>
            <device-id>jun105</device-id>
            <location>center</location>
          </device>
        </devices>
        <management>
          <type>provider-managed</type>
        </management>
        <locations>
          <location>
            <location-id>center</location-id>
          </location>
        </locations>
        <vpn-policies>
          <vpn-policy>
            <vpn-policy-id>1</vpn-policy-id>
            <entries>
              <id>1</id>
              <vpn>
                <vpn-id>45</vpn-id>
              </vpn>
            </entries>
          </vpn-policy>
        </vpn-policies>
        <default-ce-vlan-id>1</default-ce-vlan-id>
        <site-network-accesses>
          <site-network-access>
            <network-access-id>1</network-access-id>
            <device-reference>jun105</device-reference>
            <connection>
              <oam>
                <md-name>A</md-name>
                <md-level>1</md-level>
              </oam>
            </connection>
            <vpn-attachment>
              <vpn-policy-id>1</vpn-policy-id>
            </vpn-attachment>
            <service>
              <svc-mtu>1500</svc-mtu>
            </service>
          </site-network-access>
        </site-network-accesses>
      </site>
    </sites>
  </l2vpn-svc>
</config>
