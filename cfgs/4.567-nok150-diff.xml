<devices xmlns="http://tail-f.com/ns/ncs">
  <device>
    <name>nok150</name>
    <config>
      <configure xmlns="urn:nokia.com:sros:ns:yang:sr:conf">
        <service>
          <epipe>
            <service-name>netconf-test-4-5</service-name>
            <service-id>4045</service-id>
            <customer>1</customer>
            <admin-state>enable</admin-state>
            <spoke-sdp>
              <sdp-bind-id>4045:4045</sdp-bind-id>
              <admin-state>enable</admin-state>
            </spoke-sdp>
            <sap>
              <sap-id>esat-2/1/10:5</sap-id>
              <admin-state>enable</admin-state>
            </sap>
          </epipe>
          <sdp>
            <sdp-id>4045</sdp-id>
            <admin-state>enable</admin-state>
            <delivery-type>mpls</delivery-type>
            <ldp>true</ldp>
            <far-end>
              <ip-address>100.0.0.151</ip-address>
            </far-end>
          </sdp>
          <vpls>
            <service-name>netconf-test-4-7</service-name>
            <service-id>4047</service-id>
            <customer>1</customer>
            <admin-state>enable</admin-state>
            <bgp>
              <bgp-instance>1</bgp-instance>
              <route-target>
                <export>target:65000:4047</export>
                <import>target:65000:4047</import>
              </route-target>
            </bgp>
            <bgp-evpn>
              <evi>4047</evi>
              <mpls>
                <bgp-instance>1</bgp-instance>
                <admin-state>enable</admin-state>
                <auto-bind-tunnel>
                  <resolution>any</resolution>
                </auto-bind-tunnel>
              </mpls>
            </bgp-evpn>
            <sap>
              <sap-id>esat-2/1/10:7</sap-id>
              <admin-state>enable</admin-state>
            </sap>
          </vpls>
          <vprn>
            <service-name>netconf-test-4-6</service-name>
            <service-id>4046</service-id>
            <customer>1</customer>
            <admin-state>enable</admin-state>
            <route-distinguisher>auto-rd</route-distinguisher>
            <vrf-target>
              <community>target:65000:4046</community>
            </vrf-target>
            <auto-bind-tunnel>
              <resolution>any</resolution>
            </auto-bind-tunnel>
            <interface>
              <interface-name>local</interface-name>
              <admin-state>enable</admin-state>
              <ipv4>
                <primary>
                  <address>10.150.167.150</address>
                  <prefix-length>24</prefix-length>
                </primary>
              </ipv4>
              <sap>
                <sap-id>esat-2/1/10:6</sap-id>
              </sap>
            </interface>
          </vprn>
        </service>
      </configure>
    </config>
  </device>
</devices>
