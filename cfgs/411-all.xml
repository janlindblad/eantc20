<devices xmlns="http://tail-f.com/ns/ncs">
  <device>
    <name>l2vpn-controller</name>
    <config>
      <l2vpn-svc xmlns="lsa-urn:ietf:params:xml:ns:yang:ietf-l2vpn-svc">
        <vpn-services>
          <vpn-service>
            <vpn-id>45</vpn-id>
            <frame-delivery>
              <multicast-gp-port-mapping>static-mapping</multicast-gp-port-mapping>
            </frame-delivery>
            <ce-vlan-preservation>true</ce-vlan-preservation>
            <ce-vlan-cos-preservation>true</ce-vlan-cos-preservation>
          </vpn-service>
        </vpn-services>
        <sites>
          <site>
            <site-id>berlin</site-id>
            <devices>
              <device>
                <device-id>nok150</device-id>
                <location>center</location>
              </device>
            </devices>
            <management>
              <type>provider-managed</type>
            </management>
            <locations>
              <location>
                <location-id>center</location-id>
              </location>
            </locations>
            <vpn-policies>
              <vpn-policy>
                <vpn-policy-id>1</vpn-policy-id>
                <entries>
                  <id>1</id>
                  <vpn>
                    <vpn-id>45</vpn-id>
                  </vpn>
                </entries>
              </vpn-policy>
            </vpn-policies>
            <default-ce-vlan-id>1</default-ce-vlan-id>
            <site-network-accesses>
              <site-network-access>
                <network-access-id>1</network-access-id>
                <device-reference>nok150</device-reference>
                <connection>
                  <oam>
                    <md-name>1</md-name>
                    <md-level>1</md-level>
                  </oam>
                </connection>
                <vpn-attachment>
                  <vpn-policy-id>1</vpn-policy-id>
                </vpn-attachment>
                <service>
                  <svc-mtu>1500</svc-mtu>
                </service>
              </site-network-access>
            </site-network-accesses>
          </site>
          <site>
            <site-id>london</site-id>
            <devices>
              <device>
                <device-id>met134</device-id>
                <location>center</location>
              </device>
            </devices>
            <management>
              <type>provider-managed</type>
            </management>
            <locations>
              <location>
                <location-id>center</location-id>
              </location>
            </locations>
            <vpn-policies>
              <vpn-policy>
                <vpn-policy-id>1</vpn-policy-id>
                <entries>
                  <id>1</id>
                  <vpn>
                    <vpn-id>45</vpn-id>
                  </vpn>
                </entries>
              </vpn-policy>
            </vpn-policies>
            <default-ce-vlan-id>1</default-ce-vlan-id>
            <site-network-accesses>
              <site-network-access>
                <network-access-id>1</network-access-id>
                <device-reference>met134</device-reference>
                <connection>
                  <oam>
                    <md-name>A</md-name>
                    <md-level>1</md-level>
                  </oam>
                </connection>
                <vpn-attachment>
                  <vpn-policy-id>1</vpn-policy-id>
                </vpn-attachment>
                <service>
                  <svc-mtu>1500</svc-mtu>
                </service>
              </site-network-access>
            </site-network-accesses>
          </site>
        </sites>
      </l2vpn-svc>
    </config>
  </device>
</devices>
<evpn xmlns="http://example.com/eantc20/evpn">
  <sites>
    <site>
      <name>site-a</name>
      <ipv4-address>100.0.0.150</ipv4-address>
      <device>Nokia-SR1-2</device>
      <interface>1/1/c4/1:45</interface>
    </site>
    <site>
      <name>site-z</name>
      <ipv4-address>100.0.0.105</ipv4-address>
      <device>Juniper-MX204-6</device>
      <interface>xe-0/1/7.0</interface>
    </site>
  </sites>
  <connections>
    <connection>
      <name>netconf-test-4-7</name>
      <a>site-a</a>
      <z>site-z</z>
    </connection>
  </connections>
</evpn>
