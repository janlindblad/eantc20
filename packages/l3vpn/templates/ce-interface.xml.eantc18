        <!-- HUAWEI NE9k -->
        <ifm xmlns="http://www.huawei.com/netconf/vrp/huawei-ifm">
          <interfaces>
            <interface>
              <ifName>{$INTERFACE}</ifName>
              <ipv4Config>
                <addrCfgType>config</addrCfgType>
                <am4CfgAddrs>
                  <am4CfgAddr>
                    <ifIpAddr>{$IP}</ifIpAddr>
                    <subnetMask>{$MASK}</subnetMask>
                    <addrType>main</addrType>
                  </am4CfgAddr>
                </am4CfgAddrs>
              </ipv4Config>
            </interface>
          </interfaces>
        </ifm>
        <!-- END HUAWEI NE9k -->


        <!-- OTHER VENDOR -->

      </config>
    </device>
  </devices>
</config-template>
