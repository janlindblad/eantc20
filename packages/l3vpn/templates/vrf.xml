<config-template xmlns="http://tail-f.com/ns/config/1.0">
 <devices xmlns="http://tail-f.com/ns/ncs">
  <device tags="nocreate">
    <name>{$DEVICE}</name>
      <config xmlns:eci="http://tail-f.com/ns/ned-id/eci5-nc-1.0">

      <!-- NOKIA -->
      <configure xmlns="urn:nokia.com:sros:ns:yang:sr:conf">
        <service>
          <vprn>
            <service-name>{$VPN_NAME}</service-name>
            <service-id>{$VLANID}</service-id>
            <customer>1</customer>
            <admin-state>enable</admin-state>
            <route-distinguisher>auto-rd</route-distinguisher>
            <vrf-target>
              <community>target:65000:{$VLANID}</community>
            </vrf-target>
            <auto-bind-tunnel>
              <resolution>any</resolution>
            </auto-bind-tunnel>
          </vprn>
        </service>
      </configure>
      </vprn>
      <!-- END NOKIA -->

      <!-- METASWITCH -->
      <router xmlns="http://metaswitch.com/yang/nbu/routing/201512140000Z">
        <vrf>
          <vrf-name>ALLOC same as above: netconf</vrf-name>
          <import>
            <route-target>
              <rt>65000:ALLOC match other side: 20</rt>
            </route-target>
          </import>
          <export>
            <route-target>
              <rt>65000:ALLOC match above 20</rt>
            </route-target>
          </export>
          <rd xmlns="http://metaswitch.com/yang/nbu/l3vpn/201511130000Z">ALLOC unique per vrf. 10:100</rd>
        </vrf>
      </router>
      <!-- END METASWITCH -->



